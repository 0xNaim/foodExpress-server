{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "description": {
      "type": "text",
      "required": true,
      "unique": false
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "regularPrice": {
      "type": "decimal",
      "unique": false,
      "required": true
    },
    "sellPrice": {
      "type": "decimal",
      "unique": false
    },
    "stoke": {
      "type": "integer",
      "required": true,
      "unique": false
    },
    "offer": {
      "type": "boolean",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "weight": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "variant": {
      "type": "enumeration",
      "enum": [
        "daily",
        "monthly",
        "safeFood"
      ],
      "required": true,
      "default": "daily"
    }
  }
}
